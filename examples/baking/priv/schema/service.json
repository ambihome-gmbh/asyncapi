{
  "asyncapi": "3.0.0",
  "info": {
    "title": "Stack API",
    "version": "0.0.1",
    "description": "API for a stack with push and pop operations"
  },
  "defaultContentType": "application/json",
  "servers": {
    "production": {
      "host": "127.0.0.1",
      "protocol": "mqtt",
      "description": "Test broker",
      "variables": {
        "port": {
          "description": "MQTT port",
          "default": "1883",
          "enum": [
            "1883"
          ]
        }
      }
    }
  },
  "operations": {
    "start_baking": {
      "action": "receive",
      "channel": {
        "$ref": "#/channels/start_baking"
      }
    },
    "baking_done": {
      "action": "send",
      "channel": {
        "$ref": "#/channels/baking_done"
      }
    },
    "baking_not_done": {
      "action": "send",
      "channel": {
        "$ref": "#/channels/baking_not_done"
      }
    }
  }
}