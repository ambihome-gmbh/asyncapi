{
    "asyncapi": "3.0.0",
    "servers": {
        "production": {
            "host": "127.0.0.1",
            "protocol": "mqtt",
            "variables": {
                "port": {
                    "default": "1883",
                    "enum": [
                        "1883"
                    ]
                }
            }
        }
    },
    "components": {
        "schemas": {
            "empty_payload": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "_sender": {
                        "description": "sender uuid",
                        "type": "string"
                    }
                }
            }
        }
    }
}